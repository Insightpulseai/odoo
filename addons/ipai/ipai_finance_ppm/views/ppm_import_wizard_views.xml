<odoo>
  <record id="view_ipai_ppm_import_wizard_form" model="ir.ui.view">
    <field name="name">ipai.ppm.import.wizard.form</field>
    <field name="model">ipai.ppm.import.wizard</field>
    <field name="arch" type="xml">
      <form string="PPM Import">
        <group>
          <field name="filename" readonly="1"/>
          <field name="file" filename="filename"/>
          <field name="source"/>
          <field name="batch_id"/>
        </group>
        <footer>
          <button name="action_import" type="object" string="Import" class="btn-primary"/>
          <button special="cancel" string="Cancel"/>
        </footer>
      </form>
    </field>
  </record>
</odoo>
