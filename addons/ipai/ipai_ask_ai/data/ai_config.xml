<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- AI Configuration Parameters -->
        <record id="config_ai_provider" model="ir.config_parameter">
            <field name="key">ipai_ask_ai.provider</field>
            <field name="value">mock</field>
        </record>

        <record id="config_ai_api_key" model="ir.config_parameter">
            <field name="key">ipai_ask_ai.api_key</field>
            <field name="value"></field>
        </record>

        <record id="config_ai_model" model="ir.config_parameter">
            <field name="key">ipai_ask_ai.model</field>
            <field name="value">gpt-4</field>
        </record>

        <record id="config_ai_endpoint" model="ir.config_parameter">
            <field name="key">ipai_ask_ai.endpoint</field>
            <field name="value"></field>
        </record>
    </data>
</odoo>
