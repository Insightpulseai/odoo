{
  "name": "@insightpulse/docs-ai-widget",
  "version": "0.1.0",
  "description": "InsightPulse Docs AI - Embeddable documentation assistant widget",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:embed": "esbuild src/embed.ts --bundle --minify --outfile=dist/embed.min.js",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.50.0",
    "typescript": "^5.2.0",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "keywords": [
    "docs",
    "ai",
    "widget",
    "rag",
    "documentation",
    "assistant",
    "insightpulse"
  ],
  "author": "InsightPulse",
  "license": "MIT"
}
