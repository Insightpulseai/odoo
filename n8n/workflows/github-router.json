            "index": 0
          },
          {
            "node": "Log to Supabase Jobs",
            "type": "main",
            "index": 0
          }
        ],
        []
      ]
    },
    "IF Push Event": {
      "main": [
        [
          {
            "node": "Set Event Context",
            "type": "main",
            "index": 0
          }
        ],
        []
      ]
    },
    "IF PR Event": {
      "main": [
        [
          {
            "node": "Log PR to Supabase",
            "type": "main",
            "index": 0
          }
        ],
        []
      ]
