{
  "models": [
    {
      "title": "Claude Sonnet 4.5",
      "provider": "anthropic",
      "model": "claude-sonnet-4-5-20250929",
      "apiKey": "$ANTHROPIC_API_KEY"
    },
    {
      "title": "Claude Opus 4.5",
      "provider": "anthropic",
      "model": "claude-opus-4-5-20251101",
      "apiKey": "$ANTHROPIC_API_KEY"
    },
    {
      "title": "Gemini 2.5 Flash",
      "provider": "gemini",
      "model": "gemini-2.5-flash-latest",
      "apiKey": "$GEMINI_API_KEY"
    },
    {
      "title": "Gemini 2.5 Pro",
      "provider": "gemini",
      "model": "gemini-2.5-pro-latest",
      "apiKey": "$GEMINI_API_KEY"
    },
    {
      "title": "Gemini 3.0",
      "provider": "gemini",
      "model": "gemini-3.0-latest",
      "apiKey": "$GEMINI_API_KEY"
    }
  ],
  "tabAutocompleteModel": {
    "title": "Claude Sonnet 4.5",
    "provider": "anthropic",
    "model": "claude-sonnet-4-5-20250929",
    "apiKey": "$ANTHROPIC_API_KEY"
  },
  "embeddingsProvider": {
    "provider": "openai",
    "model": "text-embedding-3-small",
    "apiKey": "$OPENAI_API_KEY"
  },
  "contextProviders": [
    {
      "name": "code",
      "params": {
        "includePatterns": [
          "**/*.py",
          "**/*.xml",
          "**/*.js",
          "**/*.ts",
          "**/*.sql",
          "**/*.yaml",
          "**/*.yml"
        ],
        "excludePatterns": [
          "**/node_modules/**",
          "**/__pycache__/**",
          "**/venv/**",
          "**/dist/**",
          "**/build/**",
          "**/.git/**"
        ]
      }
    },
    {
      "name": "docs",
      "params": {
        "sites": [
          {
            "title": "Odoo 18 Documentation",
            "startUrl": "https://www.odoo.com/documentation/18.0/"
          },
          {
            "title": "OCA Guidelines",
            "startUrl": "https://github.com/OCA/maintainer-tools"
          },
          {
            "title": "Supabase Documentation",
            "startUrl": "https://supabase.com/docs"
          }
        ]
      }
    },
    {
      "name": "folder",
      "params": {
        "folders": [
          "docs/",
          "addons/ipai/ipai_tenant_core/"
        ]
      }
    },
    {
      "name": "terminal",
      "params": {}
    },
    {
      "name": "problems",
      "params": {}
    },
    {
      "name": "diff",
      "params": {}
    },
    {
      "name": "open",
      "params": {}
    },
    {
      "name": "codebase",
      "params": {
        "nRetrieve": 25,
        "useReranking": true
      }
    }
  ],
  "slashCommands": [
    {
      "name": "edit",
      "description": "Edit selected code"
    },
    {
      "name": "comment",
      "description": "Write comments for selected code"
    },
    {
      "name": "share",
      "description": "Export conversation to markdown"
    },
    {
      "name": "cmd",
      "description": "Generate shell command"
    },
    {
      "name": "commit",
      "description": "Generate git commit message"
    }
  ],
  "customCommands": [
    {
      "name": "tenant",
      "prompt": "You are an Odoo 18 multi-tenant architecture expert. Review the code in context and provide insights on:\n1. Tenant isolation (database-level and schema-level)\n2. OCA compliance\n3. Integration with Supabase and Superset\n\nRefer to docs/TENANT_ARCHITECTURE.md and addons/ipai/ipai_tenant_core/TECHNICAL_GUIDE.md for context.",
      "description": "Tenant architecture review"
    },
    {
      "name": "oca",
      "prompt": "You are an OCA compliance expert. Review the selected code for:\n1. OCA module structure standards\n2. Proper field validation and constraints\n3. Security access control patterns\n4. reStructuredText documentation completeness\n\nRefer to OCA maintainer-tools guidelines.",
      "description": "OCA compliance check"
    },
    {
      "name": "provision",
      "prompt": "You are a DevOps automation expert. Review the provisioning workflow for:\n1. Tenant setup automation\n2. Supabase schema creation\n3. Odoo database initialization\n4. Error handling and rollback procedures\n\nRefer to scripts/provision_tenant.sh and Makefile.",
      "description": "Provisioning workflow review"
    },
    {
      "name": "superset",
      "prompt": "You are a BI integration expert. Review the Superset integration for:\n1. Cross-repo CI coordination\n2. Guest token generation\n3. Embedded dashboard patterns\n4. Dataset schema synchronization\n\nRefer to docs/SUPERSET_INTEGRATION.md.",
      "description": "Superset integration review"
    }
  ],
  "allowAnonymousTelemetry": false,
  "experimental": {
    "modelRoles": {
      "inlineEdit": "Claude Sonnet 4.5",
      "applyCodeBlock": "Claude Sonnet 4.5",
      "repoMapFileSelection": "Claude Sonnet 4.5"
    }
  },
  "github": {
    "token": "$GITHUB_TOKEN"
  }
}
