{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": ["ES2022"],
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"]
    }
  },
  "include": ["server/**/*.ts", "__tests__/**/*.ts", "nitro.config.ts"],
  "exclude": ["node_modules", ".output", ".nitro"]
}
