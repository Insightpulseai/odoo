{
  "id": "keycloak",
  "name": "Keycloak SSO",
  "summary": "Self-hosted identity and access management. SAML SSO replacement for GitHub Enterprise.",
  "category": "security",
  "type": "native",
  "capabilities": ["sso", "saml", "oauth2", "user-federation", "mfa"],
  "install": {
    "provisioner": "scripts/provisioners/install_keycloak.sh",
    "scopes": [],
    "env_vars": ["KEYCLOAK_URL", "KEYCLOAK_REALM", "KEYCLOAK_CLIENT_ID", "KEYCLOAK_CLIENT_SECRET"],
    "auto_configure": true
  },
  "links": {
    "docs": "https://www.keycloak.org/documentation",
    "repo": "https://github.com/keycloak/keycloak"
  },
  "ipai_module": "ipai_auth_keycloak",
  "status": "active"
}
