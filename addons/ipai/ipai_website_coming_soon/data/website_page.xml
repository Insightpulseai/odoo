<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Register as a website page at /homepage (set as homepage via hook) -->
        <record id="page_coming_soon" model="website.page">
            <field name="name">InsightPulseAI Homepage</field>
            <field name="url">/homepage</field>
            <field name="view_id" ref="coming_soon"/>
            <field name="website_published">True</field>
            <field name="is_published">True</field>
        </record>

    </data>
</odoo>
