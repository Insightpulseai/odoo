<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Default OCR configuration parameters -->
        <record id="config_expense_ocr_provider" model="ir.config_parameter">
            <field name="key">ipai_expense_ocr.provider</field>
            <field name="value">custom</field>
        </record>

        <record id="config_expense_ocr_auto_extract" model="ir.config_parameter">
            <field name="key">ipai_expense_ocr.auto_extract</field>
            <field name="value">True</field>
        </record>

        <record id="config_expense_ocr_endpoint_url" model="ir.config_parameter">
            <field name="key">ipai_expense_ocr.endpoint_url</field>
            <field name="value"></field>
        </record>
    </data>
</odoo>
