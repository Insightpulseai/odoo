<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <!-- Compliance check definitions will be generated dynamically per period -->
    <!-- This file contains reference data for check types -->

    <!-- Sequence for closing periods -->
    <record id="seq_closing_period" model="ir.sequence">
        <field name="name">Closing Period</field>
        <field name="code">closing.period</field>
        <field name="prefix">CLOSE-%(year)s-</field>
        <field name="padding">2</field>
    </record>

    <!-- Sequence for BIR returns -->
    <record id="seq_bir_return" model="ir.sequence">
        <field name="name">BIR Return</field>
        <field name="code">bir.return</field>
        <field name="prefix">BIR-%(year)s-</field>
        <field name="padding">5</field>
    </record>
</odoo>
